{
  "name": "example-render-callback",
  "scripts": {
    "dev": "next",
    "test:watch": "npm test -- --watch",
    "verify": "npm test && npm run lint && npm run build",
    "test": "jest .",
    "lint": "eslint .",
    "build": "next build",
    "stage": "git checkout master && git pull && now",
    "release": "now alias",
    "start": "next start"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "env": {
      "es6": true,
      "node": true,
      "jest": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/display-name": "off",
      "react/prop-types": "off"
    }
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "now": {
    "alias": "example-render-callback"
  },
  "devDependencies": {
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.4",
    "now": "^6.3.1"
  },
  "dependencies": {
    "next": "^2.4.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}
